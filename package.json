{
  "name": "shekayla-maragh-site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "animate.css": "^3.7.2",
    "bootstrap": "^4.3.1",
    "node-sass": "^4.13.1",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-background-video-player": "^1.1.6",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-burgers": "^1.3.0",
    "react-dom": "^16.8.6",
    "react-native": "^0.60.4",
    "react-native-video": "^4.4.4",
    "react-parallax": "^2.2.0",
    "react-player": "^1.11.1",
    "react-responsive": "^7.0.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-router-hash-link": "^1.2.2",
    "react-scripts": "^3.3.1",
    "react-scroll-parallax": "^2.3.1",
    "react-sticky": "^6.0.3",
    "react-sticky-el": "^1.0.20",
    "react-sticky-nav": "^3.1.12",
    "reactstrap": "^8.0.1",
    "semantic-ui-react": "^0.87.3",
    "universal-parallax": "^1.3.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
